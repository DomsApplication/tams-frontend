{
  "name": "tams-frontend-application",
  "region": "blr",
  "services": [
    {
      "name": "tams-frontend",
      "github": {
        "repo": "DomsApplication/tams-frontend",
        "branch": "main",
        "deploy_on_push": true
      },
      "source_dir": "frontend",
      "build_command": "npm install && npm run build",
      "run_command": "npx serve -s build",
      "environment_slug": "node-js",
      "routes": [
        {
          "path": "/"
        }
      ]
    }
  ]
}
