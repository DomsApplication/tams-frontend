{
  "name": "tams-frontend-application",
  "services": [
    {
      "name": "tams-frontend",
      "github": {
        "branch": "main",
        "deploy_on_push": true,
        "repo": "DomsApplication/tams-frontend"
      },
      "source_dir": "frontend",
      "build_command": "npm install && npm run build",
      "run_command": "",
      "envs": [],
      "routes": [
        {
          "path": "/"
        }
      ],
      "static_sites": [
        {
          "name": "tams-frontend-static",
          "source_dir": "frontend",
          "output_dir": "build",
          "routes": [{ "path": "/" }]
        }
      ]
    }
  ],
  "region": "blr"
}
